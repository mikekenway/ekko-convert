{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "dev:server": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "!.next/cache/**", "built/**"]
        },
        "test": {
            "dependsOn": ["build"]
        },
        "lint": {}
    }
}
